{
  "name": "figma-design-system-generator",
  "version": "1.0.0",
  "description": "Configurable Figma plugin for design system generation",
  "main": "code.js",
  "scripts": {
    "build": "esbuild src/code.ts --bundle --outfile=code.js --platform=node --target=es2015 --loader:.html=text",
    "watch": "esbuild src/code.ts --bundle --outfile=code.js --platform=node --target=es2015 --loader:.html=text --watch"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.98.0",
    "esbuild": "^0.24.2",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "figma",
    "plugin",
    "design-system"
  ],
  "author": "Yumiko Fujiwara",
  "license": "MIT"
}